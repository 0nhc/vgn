{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}/vgn",
            "console": "integratedTerminal",
        },
        {
            "name": "Dataset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/vgn/scripts/generate_dataset.py",
            "cwd": "${workspaceFolder}/vgn",
            "console": "integratedTerminal",
            "args": [
                "--dataset=data/datasets/blocks",
                "--object-set=blocks",
                "--grasps=100",
                "--sim-gui",
            ]
        },
        {
            "name": "Training",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/vgn/scripts/train_vgn.py",
            "cwd": "${workspaceFolder}/vgn",
            "console": "integratedTerminal",
            "args": [
                "--dataset-dir=data/datasets/blocks",
            ]
        },
        {
            "name": "Benchmark",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/vgn/scripts/run_benchmark.py",
            "cwd": "${workspaceFolder}/vgn",
            "console": "integratedTerminal",
            "args": [
                "--model=data/models/vgn_conv_100.pth",
                "--object-set=adversarial",
                "--object-count=5",
                "--log-dir=data/experiments",
                "--sim-gui",
            ]
        },
    ]
}