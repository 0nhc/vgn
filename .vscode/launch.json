{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Current File",
            "type": "python",
            "request": "launch",
            "program": "${file}",
            "cwd": "${workspaceFolder}/vgn",
            "console": "integratedTerminal",
        },
        {
            "name": "Dataset",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/vgn/scripts/generate_dataset.py",
            "cwd": "${workspaceFolder}/vgn",
            "console": "integratedTerminal",
            "args": [
                "--dataset=data/datasets/meh",
                "--object-set=train",
                "--sim-gui",
            ]
        },
        {
            "name": "Training",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/vgn/scripts/train_vgn.py",
            "cwd": "${workspaceFolder}/vgn",
            "console": "integratedTerminal",
            "args": [
                "--dataset-dir=data/datasets/train",
                "--batch-size=32",
                "--description=meh",
            ]
        },
        {
            "name": "Evaluation (VGN)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/vgn/scripts/eval_vgn.py",
            "cwd": "${workspaceFolder}/vgn",
            "console": "integratedTerminal",
            "args": [
                "--model=",
                "--object-set=blocks",
                "--logdir=data/experiments/meh/vgn",
                "--sim-gui",
            ]
        },
        {
            "name": "Evaluation (GPD)",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/vgn/scripts/eval_gpd.py",
            "cwd": "${workspaceFolder}/vgn",
            "console": "integratedTerminal",
            "args": [
                "--object-set=blocks",
                "--logdir=data/experiments/meh/gpd",
                "--sim-gui",
            ]
        },
    ]
}