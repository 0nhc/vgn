{
    "editor.formatOnSave": true,
    "files.exclude": {
        "**/*.pyc": true,
    },
    "python.pythonPath": "venv/bin/python",
    "python.formatting.provider": "yapf",
    "python.autoComplete.extraPaths": [
        "./vgn/src",
        "./vgn_ros/src"
    ],
    "[python]": {
        "editor.codeActionsOnSave": {
            "source.organizeImports": true
        }
    }
}