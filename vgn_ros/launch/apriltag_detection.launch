<launch>
  <arg name="node_namespace" default="apriltag_ros" />

  <rosparam command="load" file="$(find vgn_ros)/config/apriltag_settings.yaml" ns="$(arg node_namespace)" />

  <node pkg="apriltag_ros" type="apriltag_ros_continuous_node" name="$(arg node_namespace)" clear_params="true" output="screen">
    <remap from="image_rect" to="/camera/rgb/image_rect_color" />
    <remap from="camera_info" to="/camera/rgb/camera_info" />
    <param name="camera_frame" type="str" value="camera_rgb_optical_frame" />
    <param name="publish_tag_detections_image" type="bool" value="false" />
  </node>
</launch>
